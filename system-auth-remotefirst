# Remote first authentication order.

auth required pam_env.so debug
auth [default=ignore success=1] pam_succeed_if.so uid = 0 debug
auth [default=ignore success=1] pam_succeed_if.so uid != 0 debug
auth [success=ok new_authtok_reqd=done default=die] pam_unix.so likeauth nullok try_first_pass debug
auth [success=done new_authtok_reqd=done authinfo_unavail=ignore default=ignore] pam_tacplus.so encrypt try_first_pass debug
auth sufficient pam_unix.so likeauth try_first_pass debug
auth sufficient pam_radius_auth.so use_first_pass debug
auth required pam_deny.so debug

account required pam_unix.so debug
account sufficient pam_tacplus.so encrypt service=raccess protocol=unknown debug

password sufficient pam_unix.so nullok md5 shadow debug
password required pam_deny.so debug

session sufficient pam_tacplus.so service=ppp protocol=lcp debug
session required pam_limits.so debug
session required pam_unix.so nullok debug

